metavar var, X ::=
  {{ com term variable }}
  {{ coq nat }}
  {{ ocaml string }}
  {{ lex alphanum }}

grammar
term, t :: 't_' ::= {{ menhir-start }}
  | X              :: :: var
  | [ X ]          :: :: list
  | ( t )          :: :: paren

atomic_term, A :: 'atomic_' ::=
  | X              :: :: var
  | [ X ]          :: :: list

subrules
  A <:: t
